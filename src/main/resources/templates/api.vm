#foreach ($apiEntry in $apiMap)
    #foreach( $api in $apiEntry.value)
    <div>
        <h2>namespace : $apiEntry.key</h2>
            <h3>API name:${api.name}</h3>
            <span class="apiFullPath">API full path:${api.fullPath}</span>

            <div class="comment_block">
                    ${api.block}
            </div>
            <pre>
                <code>
                #foreach ( $param in ${api.params})
                        ${param.name}:${param.tp} ${param.required}
                #end
                </code>
            </pre>

            <div class="returns_list">
                <span>返回结果</span>
                <ul class="returns_list">
                    #foreach( $return in $returns )
                        <li>
                            <span>
								$return.text
                            </span>
                        </li>
                    #end

                </ul>
            </div>
        </li>
	#end
</div>
#end

